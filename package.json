{
  "name": "m163up",
  "version": "1.0.0",
  "description": "网易云音乐音质替换检查小组任务公示栏",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "if [[ ${NODE_ENV} == \"production\" ]]; then npm run build:prod; else npm run build:dev; fi",
    "build:dev": "webpack --define process.env.NODE_ENV='\"development\"' --progress --colors && jekyll b",
    "build:prod": "webpack -p --define process.env.NODE_ENV='\"production\"' --progress --colors && jekyll b",
    "start": "if [[ ${NODE_ENV} == \"production\" ]]; then npm run start:prod; else npm run start:dev; fi",
    "start:dev": "webpack --define process.env.NODE_ENV='\"development\"' --watch --progress --colors & jekyll s -o",
    "start:prod": "webpack -p --define process.env.NODE_ENV='\"production\"' --watch --progress --colors & jekyll s -o"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hufan-Akari/M163UP.git"
  },
  "author": "阿卡琳",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/hufan-Akari/M163UP/issues"
  },
  "homepage": "https://github.com/hufan-Akari/M163UP#readme",
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "localforage": "^1.4.2",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-immutablejs": "^0.0.8",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.3"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-3": "^6.11.0",
    "webpack": "^1.13.2"
  }
}